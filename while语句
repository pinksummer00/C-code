#include<stdio.h>
int main()
{
	int i = 1;
	while (i <= 10)
	{
		if (i == 5)
			break; //终止整个循环
		printf("%d\n", i);
		i++;
	}
	return 0;
}



#include <stdio.h>
int main()
{
	int i = 1;
	while (i <= 10)
	{
		if (i == 5)
			continue;//终止本次循环后面的内容，跳出循环，重新进行下一次入口的判断
		printf("%d\n", i);
		i++;
	}
	return 0;
}
