#include <stdio.h>
#include<stdlib.h>
#include <string.h>
#include<errno.h>
int main()
{
	int* p = (int*)calloc(10,sizeof(int));//calloc函数会初始化每个空间为0
	if (p == NULL)
	{
		printf("%s\n", strerror(errno));//打印错误原因的一个方式
	}
	else
	{
		int i = 0;
		for (i = 0;i < 10;i++)
		{
			printf("%d ", *(p + i));
		}
	}
	free(p);//free函数是用来释放动态空间的
	p = NULL;
	return 0;
}
