#include <stdio.h>
void test()
{
	static int a = 1;//静态的局部变量
    a++;
	printf("a=%d\n", a);
}
int main()
{
	int i = 0;
	while (i < 5)
	{
		test();
		i++;
	}
	return 0;
}


int Add(int x, int y)
{
	int z = x + y;
	return z;
}
extern int Add(int, int);// extern 是用来声明外部符号的
int main()
{
	int a = 10;
	int b = 20;
	int sum = Add(a, b);
	printf("sum=%d\n", sum);
	return 0;
}
