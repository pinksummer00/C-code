#include<stdio.h>
int main()
{
	int year = 0;
	int count = 0;
	for (year = 1000; year <= 2000; year++) //打印1000-2000年之间的闰年
	{
		if (year % 4 == 0 && year % 100 != 0) //能被4整除并且不能被100整除的是闰年
		{
			printf("%d\n", year);
			count++;
		}
		else if (year % 400 == 0) //能被400整除的也是闰年
		{
			printf("%d\n", year);
			count++;
		}
	}
	printf("\ncount=%d\n", count);
	return 0;
}
