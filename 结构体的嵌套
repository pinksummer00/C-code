#include<stdio.h>
struct S
{
	int a;
	char c;
	char arr[20];
	double d;
};
struct T
{
	char ch[10];
	struct S s;//结构体的嵌套
	char* pc;
};
int main()
{
	char arr[] = "hello bit\n";
	struct T t = { "hehe",{100,'w',"hello word",3.14},arr };
	printf("%s\n", t.ch);
	printf("%s\n", t.s.arr);
	printf("%lf\n", t.s.d);
	printf("%s\n", t.pc);
	return 0;
}
