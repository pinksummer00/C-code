#include<stdio.h>
int main()
{
	int i = 0;
	int count = 0;
	for (i = 100; i <= 200; i++) //素数判断的规则：试除法
	{
		int j = 0;
		for (j = 2; j < i; j++)
		{
			if (i % j == 0) //素数是只能被1和它本身整除
			{
				break;
			}
		}
		if (j == i)
		{
			count++;
			printf("%d\n", i); //打印素数
		}
	}
	printf("\ncount=%d\n", count);
	return 0;
}




#include<stdio.h>
#include<math.h>
int main()
{
	int i = 0;
	int count = 0;
	for (i = 101; i <= 200; i+=2) // 打印素数 优化
	{
		int j = 0;
		for (j = 2; j < sqrt(i); j++)
		{
			if (i % j == 0)
			{
				break;
			}
		}
		if (j > sqrt(i))
		{
			count++;
			printf("%d\n", i);
		}
	}
	printf("\ncount=%d\n", count);
	return 0;
}
